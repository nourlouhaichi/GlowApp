{% extends 'front/basefrontone.html.twig' %}

{% block title %}Objectif index{% endblock %}

{% block body %}
    <h1>Objectif index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>programme_id</th>
                {# <th>Cin</th> #}
                <th>Objectif_o</th>
                <th>Description_o</th>
                <th>Categorie_o</th>
                <th>Poid_o</th>
                <th>Taille_o</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for objectif in objectifs %}
            <tr>
                <td>{{ objectif.id }}</td>
                <td>{{objectif.programme_id}}</td>
                {# <td>{{ objectif.cin }}</td> #}
                <td>{{ objectif.objectifO }}</td>
                <td>{{ objectif.descriptionO }}</td>
                <td>{{ objectif.categorieO }}</td>
                <td>{{ objectif.poidO }}</td>
                <td>{{ objectif.tailleO }}</td>
                <td>
                    <a href="{{ path('app_objectif_show', {'id': objectif.id}) }}">show</a>
                    <a href="{{ path('app_objectif_edit', {'id': objectif.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_objectif_new') }}">Create new</a>
{% endblock %}
